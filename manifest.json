{
  "manifest_version": 3,
  "name": "Sticky Notes",
  "version": "1.0",
  "description": "A simple sticky note extension.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
